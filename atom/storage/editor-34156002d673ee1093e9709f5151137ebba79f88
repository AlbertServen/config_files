'),\n          errors     = require('../../lib/errors'),\n\n          modelPath        = '../../common/model/',\n          BHAccount        = require(`${modelPath}BHAccount`).model(),\n          TimeInterval     = require(`${modelPath}TimeInterval`).model(),\n          Treatment        = require(`${modelPath}Treatment`).model().Model,\n          TreatmentBooking = require(`${modelPath}TreatmentBooking`).model().Model,\n          Web              = require(`${modelPath}Web`).model().Model,\n\n          Crypt = require('../../common/utils/crypt.js' ),\n\n          ReqClientController = require('../../common/controller/ClientController');\n\n\n    function createCenter(req, res) {\n        const secretId = '5602d04c33866c81f2982fcf';\n        let url;\n        if (config.environment === 'development') {\n            process.env.NODE_TLS_REJECT_UNAUTHORIZED = 0;\n            url = `https://localhost:5400/api/accounts/${secretId}`;\n        } else {\n            url = `https://www.bellahora.com/api/accounts/${secretId}`;\n        }\n        request.post({\n                url,\n                form: req.body\n            }, (err, response, body) => {\n                if (err) {\n                    return errors.serverError(err, res);\n                }\n                const result = JSON.parse(body);\n        // Now only groupalia uses it, so we can hardcode this\n                BHAccount.findById(result.id_account, (err, center) => {\n                    if (err) {\n                        return errors.serverError(err, res);\n                    }\n                    BHAccount.update(\n                            {_id: result.id_account},\n                            {$set: {apiProvider: '462336d0-99c8-11e5-98a5-b994fcc2256e'}},\n                            (err) => {\n                        if (err) {\n                            return errors.serverError(err, res);\n                        }\n                        return res.json({\n                           