2], h[0], h[1]),\n\n              dateNow = new Date(),\n\n              userData = {\n                  name    : req.body.name,\n                  lastname: req.body.lastname,\n                  phone   : req.body.phone,\n                  email   : req.body.email\n              },\n              coupon     = req.body.coupon,\n              key        = req.consumer.id;\n\n        dateNow.setDate(dateNow.getDate() - 1);\n        if (date < dateNow) {\n            return errors.badRequest('Invalid date, must be greater than current time', res);\n        }\n        let treatments = req.body.treatmentIds,\n            filter     = {\n                _idBHAccount: idCenter,\n                $or: [{email: userData.email}, {emailHidden: userData.email}]\n            };\n        if (_.isEmpty(treatments) ||\n                (!validators.checkArrayIds(treatments) &&\n                !validators.checkMongoId(treatments))) {\n            return errors.badRequest('Bad request: treatment/s  required', res);\n        }\n        if (!_.isArray(treatments)) {\n            treatments = [treatments];\n        }\n        if (userData.phone) {\n            filter.$or.push({phone: userData.phone});\n        }\n\n        // Check that BHAccount and Treatment exists\n        async.parallel({\n            bhaccount : (callback) => BHAccount.findOne({_id: idCenter}, callback),\n            treatments: (callback) => {\n                Treatment.find({_id: {$in: treatments}, _idBHAccount: idCenter}, callback);\n            }\n        }, (err, results) => {\n            if (err) {\n                return errors.serverError(err, res);\n            }\n            if (results.bhaccount === null ||\n                    _.isEmpty(results.treatments) ||\n                    results.treatments.length !== _.uniq(treatments).length) {\n                return errors.notFound('BHAccount or treatment/s not found', res);\n            }\n            TreatmentBooking.findOne({\n                    _id: results.treatments[0].idTreatmentBooking,\n                    _idBHAccount: results.bhaccount._id\n                }, (err, tb) => {\n                    if (err) {\n                        return errors.serverError(err, res);\n                    }\n                    // TODO: think if TreatmentBooking === null -> Do something\n                    let realDate = new Date(d[0], d[1] - 1, d[2], h[0], h[1]);\n                    if (helpers.book.tooLate(realDate, tb.timeBeforeBooking)) {\n                        return errors.badRequest(\n                            `This treatment needs to be booked before ${tb.timeBeforeBooking} minutes`,\n                            res);\n                    }\n                    helpers.book.getClient(filter, userData, clientController)\n                        .then((client) => {\n                            helpers.book\n                                .createWork(key, client._id, date, treatments, coupon, req)\n                                .then((result) => res.json(result))\n                                .catch((err) => res.status(err.code).json(err));\n                        })\n                        .catch((err) => {\n                            return errors.serverError(err, res);\n                        });\n                });\n        });\n    }\n\n    function login(req, res) {\n        const _id = req.params.idCenter;\n        BHAccount.findOne({_id}, (err, center) => {\n            if (err) {\n                return errors.serverError(err, res);\n            }\n            if (center === null) {\n                return errors.notFound('Center not found', res);\n            }\n            const TAIL   = '542d66d6e805ad',\n                key      = req.consumer.id,\n                expire   = new Date().add({ minutes: 10 }).getTime(),\n                pwdCrypt = key + _id + TAIL,\n                crypt    = new Crypt(pwdCrypt),\n                result   = crypt.encrypt(`${expire}:bh`);\n            res.status(200).json({token: result});\n        });\n    }\n\n    function linkCenter(req, res) {\n        BHAccount.findOne({path: req.body.path}, (err, account) => {\n            if (err) {\n                return errors.serverError(err, res);\n            }\n            if (account === null) {\n                return errors.notFound('Account not found', res);\n            }\n            if (!account.apiProvider) {\n                account.apiProvider = req.consumer.id;\n                account.save((err) => {\n                    if (err) {\n                        return errors.serverError(err, res);\n                    }\n                    res.json({id: account._id.toString()});\n                });\n            } else if (account.apiProvider === req.consumer.id) {\n                return res.json({id: account._id.toString()});\n            } else {\n                return res.status(409).json({code: 409, message: 'Already linked account'});\n            }\n        });\n    }\n\n    function agreement(req, res) {\n        const _id        = req.params.idCenter,\n              onPay      = req.body.onlinePayment,\n              cc         = req.body.CC;\n              //bankRegExp = /^([A-Z]{2}\\d\\d)(\\d{4})(\\d{4})(\\d\\d)(\\d{10})$/;\n        //let ccMatch    = cc.match(bankRegExp),\n        let commission = req.body.commission || 20;\n        // if (ccMatch === null) {\n        //     return res.status(400).json({code: 400, message: 'Bad bank account format'});\n        // }\n        if (onPay && (!req.body.commission)) {\n            return errors.badRequest('Missing commission required', res);\n        }\n        // commission = parseInt(commission, 10);\n        BHAccount.findOne({_id}, (err, account) => {\n            if (err) {\n                return errors.serverError(err, res);\n            }\n            if (account === null) {\n                return errors.notFound('Account not found', res);\n            }\n            if ((account.accountType === 'premium' || account.accountType === 'gold') &&\n                    onPay && (!cc)) {\n                return errors.badRequest('Missing cc for premium', res);\n            }\n            // let bankAccount2 = {\n            //     bank   : ccMatch[2],\n            //     office : ccMatch[3],\n            //     dc     : ccMatch[4],\n            //     number : ccMatch[5],\n            //     iban   : ccMatch[1]\n            // }\n            if (cc) {\n                if (!account.bankAccount) {\n                    account.bankAccount = {};\n                }\n                account.bankAccount.iban = cc;\n            }\n            account.onPayAgreement = onPay;\n            account.commission     = commission;\n            account.waysToPay      = onPay? ['creditcard']: ['direct'];\n            account.save((err) => {\n                if (err) {\n                    return errors.serverError(err, res);\n                }\n                res.status(204).send();\n            });\n        });\n    }\n\n    return {\n        createCenter,\n        getCenter,\n        getCenterTreatments,\n        book,\n        login,\n        linkCenter,\n        agreement\n    };\n}\n\nmodule.exports = controller();\n","defaultMarkerLayerId":"0","markerLayers":{"0":{"id":"0","maintainHistory":false,"markersById":{"2":{"range":{"start":{"row":0,"column":0},"end":{"row":0,"column":0}},"properties":{},"reversed":false,"tailed":true,"valid":true,"invalidate":"overlap"},"4":{"range":{"start":{"row":0,"column":0},"end":{"row":0,"column":23}},"properties":{},"reversed":false,"tailed":true,"valid":true,"invalidate":"inside"},"5":{"range":{"start":{"row":170,"column":102},"end":{"row":170,"column":103}},"properties":{},"reversed":false,"tailed":true,"valid":true,"invalidate":"inside"},"6":{"range":{"start":{"row":5,"column":10},"end":{"row":5,"column":44}},"properties":{},"reversed":false,"tailed":true,"valid":true,"invalidate":"inside"}},"version":2},"1":{"id":"1","maintainHistory":false,"