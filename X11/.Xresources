Xcursor.theme: Capitaine Cursors

! Desktop
Xft.antialias: 1
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.dpi: 100
Xft.rgba: rgb
Xft.lcdfilter: lcddefault
Xcursor.size: 8
Xft.autohint: 1

!URxvt.iso14755_52: false

!xscreensaver.newLoginCommand: dm-tool switch-to-greeter
URxvt*termName: xterm-256color

!URxvt.colorBD:#ffffff
!URxvt.colorUL:#ffffff

URxvt.geometry:100x30
! Disable scrollbar
URxvt.scrollBar:false
! Set the font
URxvt.cursorUnderline:true
URxvt.buffered: true
URxvt.cursorBlink:true
URxvt.pointerBlank:true
URxvt.letterSpace: -1
URxvt.font: xft:Monoid Nerd Font Mono:style=Regular:pixelsize=12:antialias=true
!,xft:Monoisome:pixelsize=14:antialias=true
!,\
! xft:Monoisome:style=Regular:size=10
!URxvt.font: xft:Dejavu Sans Mono for Powerline:style=Regular:size=8
! URxvt.boldFont: xft:ypn monospace:size=8

!URxvt.deletekey:^[[3~
URxvt.saveLines:65535
URxvt.utmpInhibit:true
URxvt.skipBuiltinGlyphs:true
URxvt.perl-ext:
URxvt.perl-ext-common:default,clipboard,resize-font,matcher,url-select
URxvt.url-launcher:/usr/bin/iceweasel
URxvt.matcher.button:1
URxvt.urgentOnBell: true

! URxvt.transparent: true
! URxvt.shading:20
URxvt*depth: 32
URxvt*background: rgba:1100/1100/1100/bb00

URxvt*fading: 20
! url selection
URxvt.keysym.M-u: perl:url-select:select_next
! Set terminal padding
URxvt.internalBorder: 5
! Enables copy/paste with Ctrl-Shift-C/V DEPRECATED!
URxvt.iso14755: false

! brogrammer
! special
*.foreground:  #b3c9d7
*.background:   #191919
*.cursorColor:  #00ff00


! black, red, green, yellow, blue, purple, blue, white
*.color0:      #2c2c2c
*.color1:      #c62828
!*.color2:      #558b2e
!*.color2:      #259e83
